{
    "name": "getsky/phalcon-bootstrap",
    "type": "library",
    "description": "Bootstrap Component for Phalcon",
    "keywords": [ "framework", "phalcon", "bootstrap" ],
    "license": "MIT",
    "authors": [
        {
            "name": "JimmDiGrizli",
            "email": "getmansk-y@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-phalcon": ">=1.2.4,<2",

        "getsky/phalcon-autoload-services": "1.1.*",
        "getsky/phalcon-config-loader": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "1.*",
        "codeception/codeception": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetSky\\Phalcon\\Bootstrap\\": "src/"
        }
    }
}
